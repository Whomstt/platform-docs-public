    "rawmdx:export": "tsx src/scripts/export-raw-mdx.ts",
    "cf:dev": "pnpm cf:build && npx wrangler pages dev --port 3000 --log-level=debug",
    "postinstall": "git submodule update --init --recursive && pnpm -s build-api-docs",
    "copy-assets-file": "tsx src/scripts/copy-assets-file.ts",
    "prebuild": "pnpm -s cookbook:build && pnpm -s rawmdx:export && pnpm search:build && pnpm -s copy-assets-file",
    "build-api-docs": "docs-md"
